	public void setUp() {
		service = new CalculatorService() {
			
			@Override
			public int add(int i, int j) {
				// TODO Auto-generated method stub
				return 0;
			}
		};
	}
	public void testAdd() {
		assertEquals(10, service.add(2, 3));
	}
}
